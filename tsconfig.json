{
	"compilerOptions": {
		"module": "Node16",
		"target": "ES2022",
		"outDir": "out",
		"lib": [
			"ES2022"
		],
		"jsx": "react-jsx",
		"sourceMap": true,
		"rootDir": "src",
		"strict": true ,
		"paths": {
      "react": ["./node_modules/@types/react"],
    },
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true, 
		"noUnusedParameters": true, 
	}
}
